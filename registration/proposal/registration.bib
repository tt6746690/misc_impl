	file = {Vercauteren et al_2008_Diffeomorphic Demons - Efficient Non-parametric Image Registration.pdf:/Users/wpq/Dropbox (MIT)/zotero/Vercauteren et al_2008_Diffeomorphic Demons - Efficient Non-parametric Image Registration.pdf:application/pdf}
